# Server Configuration
NODE_ENV=development # no need to change
PORT=5000 # no need to change

# Database Configuration
DB_HOST=localhost # no need to change
DB_PORT=3306 # no need to change
DB_USER=your_username
DB_PASSWORD=your_password
DB_DATABASE=roboflow_clone  # no need to change

# JWT Configuration (for authentication later)
JWT_SECRET=your_jwt_secret_key  # no need to change
JWT_EXPIRES_IN=86400   # no need to change

# AWS S3 Configuration (for file uploads later)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1 # no need to change
S3_BUCKET=roboflow-clone-images # no need to change